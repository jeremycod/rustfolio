{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            calculation_status,\n            calculated_at,\n            last_error,\n            retry_count,\n            EXTRACT(EPOCH FROM (NOW() - calculated_at))::BIGINT as age_seconds\n        FROM portfolio_risk_cache\n        WHERE portfolio_id = $1\n        ORDER BY calculated_at DESC\n        LIMIT 1\n        ",
