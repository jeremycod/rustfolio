  "query": "\n        SELECT\n            id,\n            job_name,\n            started_at::TEXT as \"started_at!\",\n            completed_at::TEXT as \"completed_at?\",\n            status::TEXT as \"status!\",\n            error_message,\n            items_processed,\n            items_failed,\n            duration_ms\n        FROM job_runs\n        WHERE job_name = $1\n        ORDER BY started_at DESC\n        LIMIT 100\n        ",
